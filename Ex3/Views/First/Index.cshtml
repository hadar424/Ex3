
@{
    ViewBag.Title = "Index";
}

<html>
<head>
    <link href="~/Content/IndexSheet.css" rel="stylesheet" type="text/css" />

</head>

<body>
    <canvas id="myCanvas" width="1024" height="700"> </canvas>

    <script>
        var c = document.getElementById("myCanvas");
       
        var ctx = c.getContext("2d");
        ctx.beginPath();
        ctx.arc(300, 80, 5, 0, 2 * Math.PI);
        ctx.strokeStyle = 'black';
        ctx.fillStyle = 'red';
        ctx.fill();
        ctx.stroke();
    </script>

</body>
</html>
